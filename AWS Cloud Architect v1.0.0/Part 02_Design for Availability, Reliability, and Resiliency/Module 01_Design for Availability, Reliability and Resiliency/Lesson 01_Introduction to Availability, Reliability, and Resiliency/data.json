              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2020/March/5e81848b_history-of-highly-available-platforms-/history-of-highly-available-platforms-.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/dbf0b72b-35e8-4156-a198-1520362640ef",
