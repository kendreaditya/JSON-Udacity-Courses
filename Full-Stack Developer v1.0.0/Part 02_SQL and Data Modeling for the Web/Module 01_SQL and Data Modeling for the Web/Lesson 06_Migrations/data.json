            {
              "id": 907730,
              "key": "077b4b87-9e8c-42dc-b856-e4c2d07bb979",
              "title": "ND004 C01 L06 02 Migrations Pt 2",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "j1_unrCqU7k",
                "china_cdn_id": "j1_unrCqU7k.mp4"
